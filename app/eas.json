{
    "cli": { "version": ">= 13.0.0" },
    "build": {
      "dev": {
        "developmentClient": true,
        "android": { "buildType": "apk" },
        "ios": { "simulator": true },
        "credentialsSource": "remote",
        "env": {
          "ENVFILE": ".env.development"
        }
      },
      "stage": {
        "developmentClient": true,
        "distribution": "internal",
        "android": { "buildType": "apk" },
        "ios": { "simulator": true },
        "credentialsSource": "remote",
        "env": {
          "ENVFILE": ".env.development"
        }
      },
      "production": {
        "developmentClient": false,
        "autoIncrement": true,
        "distribution": "internal",
        "android": { "buildType": "apk" },
        "credentialsSource": "remote",
        "env": {
          "ENVFILE": ".env.production"
        }
      }
    }
}  